                <FIELD NAME="showclassplan" TYPE="int" LENGTH="2" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
        <TABLE NAME="hittrainingsnachweis_entry" COMMENT="Defines hittrainingsnachweis_entry">
            <FIELDS>
